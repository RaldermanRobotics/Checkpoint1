<?xml version="1.0" ?>
 
<robot name="my_rb1_robot">
    <-- Link - chassis -->
    <link name="base_link>

        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="25"/>
            <inertia ixx="100" ixy="0" ixz="0" iyy="100" iyz="0" izz="100"/>
        </inertial>
 
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <cylinder length=".30" radius=".25"/>
            </geometry>
            <material name="Cyan">
                <color rgba=0 1 0 1"/>
            </material>
        </visual>

        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <cylinder radius=".25" length =".30"/>
            </geometry>
        </collision>
        
    </link>